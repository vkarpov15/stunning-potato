{
  "dependencies": {
    "highlight.js": "9.12.0",
    "marked": "0.5.1",
    "query-string": "6.2.0",
    "unfetch": "4.0.1"
  },
  "devDependencies": {
    "eslint": "5.3.0",
    "express": "4.16.3",
    "express-static": "1.2.6",
    "mocha": "5.2.0",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.1"
  },
  "scripts": {
    "compile": "webpack",
    "lint": "eslint .",
    "serve": "node ./static",
    "test": "mocha ./test/*.test.js"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "comma-style": "error",
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": 2
        }
      ],
      "no-constant-condition": "off",
      "keyword-spacing": "error",
      "no-buffer-constructor": "error",
      "no-console": "off",
      "no-multi-spaces": "error",
      "func-call-spacing": "error",
      "no-trailing-spaces": "error",
      "quotes": [
        "error",
        "single"
      ],
      "semi": "error",
      "space-before-blocks": "error",
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "space-infix-ops": "error",
      "space-unary-ops": "error",
      "no-var": "error",
      "prefer-const": "error"
    }
  }
}
